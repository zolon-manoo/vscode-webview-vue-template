{
  "name": "@vue-webview/libs",
  "version": "1.0.0",
  "description": "Shared libs code for Vue Webview Workspace",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "prepublishOnly": "pnpm run compile",
    "lint": "eslint \"src/**/*.{ts,vue}\"",
    "compile": "tsc -p ./tsconfig.json --skipLibCheck",
    "watch": "tsc -w -p ./tsconfig.json --skipLibCheck",
    "clean": "rm -rf ./dist"
  },
  "keywords": [
    "vscode",
    "webview",
    "shared"
  ],
  "author": "Your Name",
  "license": "MIT"
}
