{
  "name": "<%= packageName %>",
  "version": "1.0.0",
  "publisher": "<%= publisherName %>",
  "description": "<%= description %>",
  "main": "./dist/extension.js",
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,vue}\"",
    "compile": "tsc -p ./tsconfig.json --skipLibCheck",
    "watch": "tsc -w -p ./tsconfig.json --skipLibCheck",
    "clean": "rm -rf ./dist",
    "package": "vsce package --no-dependencies --no-git-tag-version"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "<%= viewId %>",
          "title": "<%= displayName %>",
          "icon": "resources/icon.png"
        }
      ]
    },
    "views": {
      "<%= viewId %>": [
        {
          "type": "webview",
          "id": "<%= viewId %>",
          "name": "<%= displayName %>",
          "icon": "resources/icon.png",
          "contextualTitle": "<%= displayName %>"
        }
      ]
    },
    "commands": [
      {
        "command": "<%= commandName %>",
        "title": "<%= commandTitle %>"
      }
    ]
  },
  "dependencies": {
    "@<%= extensionName %>/libs": "workspace:*",
    "@<%= extensionName %>/view": "workspace:*"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^2.15.0",
    "typescript": "^4.8.4"
  }
}
